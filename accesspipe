def checkout(){
    dir(BUILD_ID){
      checkout scmGit(branches: [[name: '*/test']], extensions: [], userRemoteConfigs: [[credentialsId: 'git-ed', url: 'https://github.com/bhanunew9910/awesone-repository.git']])
}
}
node{
  stages{
    stage(checkout form git){
      checkout()
}
    stage(making a file){
      sh "touch file.txt"
}
}
}
